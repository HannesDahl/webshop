<% for(let i = 0; i < products.length; i++) { %>
<% let productName = products[i].name.replace(/ /g, "_")%>
<div class="col s3 card-container">
    <div class="card">
        <a href="javascript:fetchData('ajax//p/<%=productName%>')">
            <div class="card-image">
                <img class="image" src="../images/<%=products[i].image%>">
            </div>
            <div class="card-content">
                <span class="card-title"><%=products[i].name%></span>
                <p class="card-desc"><%=products[i].description%></p>
            </div>

        </a>
        <div class="card-action">
            <a class="btn grey darken-3 white-text" href="#">Add to cart</a>
            <p class="price" value=<%=products[i].price%>><%=products[i].price%>â‚¬</p>
        </div>
    </div>
</div>
<% } %>